autocmd FileType python set tabstop=4 | set expandtab | set autoindent

set number
set cursorline
set wrap
set showcmd
set wildmenu
colorscheme koehler

set hlsearch
set incsearch
set ignorecase

noremap i k
noremap k j
noremap j h
noremap h i
noremap H I
noremap I 5k
noremap K 5j

noremap S :w<CR>
noremap Q :q<CR>

